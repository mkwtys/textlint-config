{
  "plugins": [
    "html"
  ],
  "rules": {
    "incremental-headers": true,
    "max-ten": {
      "max": 3
    },
    "no-doubled-joshi": {
      "min_interval": 1
    },
    "no-double-negative-ja": true,
    "no-dropping-the-ra": true,
    "no-mix-dearu-desumasu": true,
    "no-start-duplicated-conjunction": {
      "interval": 2
    },
    "preset-jtf-style": {
      "2.1.2.漢字": true,
      "2.1.5.カタカナ": true,
      "2.1.6.カタカナの長音": true,
      "2.2.1.ひらがなと漢字の使い分け": true
    },
    "spellcheck-tech-word": true
  }
}
